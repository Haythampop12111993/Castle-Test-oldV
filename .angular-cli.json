{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "cred"
  },
  "apps": [
    {
      "name": "cred",
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "img",
        ".htaccess",
        {
          "glob": "**/*",
          "input": "../node_modules/tinymce",
          "output": "./tinymce"
        },
        {
          "glob": "*.png",
          "input": "./versions/cred",
          "output": "assets/logos"
        },
        {
          "glob": "mainfest.json",
          "input": "./versions/cred",
          "output": "."
        },
        {
          "glob": "favicon.ico",
          "input": "./versions/cred",
          "output": "."
        },
        {
          "glob": "firebase-messaging-sw.js",
          "input": "./versions/cred",
          "output": "."
        }
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/ngx-toastr/toastr.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/primeicons/primeicons.css",
        "../node_modules/primeng/resources/themes/omega/theme.css",
        "../node_modules/primeng/resources/primeng.min.css",
        "../node_modules/@fullcalendar/common/main.min.css",
        "../node_modules/@fullcalendar/daygrid/main.min.css",
        "../node_modules/tinymce/skins/ui/oxide/skin.min.css"
      ],
      "scripts": [
        "../node_modules/chart.js/dist/Chart.min.js",
        "../node_modules/hashwords/dist/hashwords.min.js",
        "../node_modules/pusher-js/dist/web/pusher.min.js",
        "../node_modules/fingerprintjs2/dist/fingerprint2.min.js",
        "../node_modules/tinymce/tinymce.min.js",
        "assets/js/getLocalIP.js",
        "assets/js/tafqeet.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/cred/environment.ts",
        "prod": "environments/cred/environment.prod.ts",
        "staging": "environments/cred/environment.staging.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
